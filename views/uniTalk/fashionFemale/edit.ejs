<% include ../../partials/header %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">Edit <%= fashionFemale.title %></h1>
        <div style="width: 66.667%; margin: 25px auto;">
            <form action="/uniTalk/fashionFemale/<%= fashionFemale._id %>?_method=PUT" method="POST">
                <div class="form-group">
                    <label>Title</label>
                    <input class="form-control" type="text" name="fashionFemale[title]" value="<%= fashionFemale.title %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="fashionFemale[image]" value="<%= fashionFemale.image %>">
                </div>
                <div class="form-group">
                    <label>Content</label>
                    <textarea class="form-control" rows="10" name="fashionFemale[body]"><%= fashionFemale.body %></textarea>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>
                </div>
            </form>
            <a href="/uniTalk/fashionFemale/<%= fashionFemale._id %>">Go Back</a>
        </div>
    </div>
</div>
<% include ../../partials/footer %>