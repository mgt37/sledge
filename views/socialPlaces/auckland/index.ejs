                iconImage: '/images/mapMarkers/dancinghall.png',
                content: '<a href="http://inkbar.co.nz/" target="_blank"><p>Ink Bar</p></a>'
            },{
                coords: {lat: -36.857802, lng: 174.758667},
                iconImage: '/images/mapMarkers/dancinghall.png',
                content: '<a href="http://www.kroad.com/nightlife/nightclubs/" target="_blank"><p>Le Box</p></a>'
            },{
                coords: {lat: -36.857714, lng: 174.760689},
                iconImage: '/images/mapMarkers/dancinghall.png',
                content: '<a href="http://www.neckofthewoods.co.nz/" target="_blank"><p>Neck of The Woods</p></a>'
            },{
                coords: {lat: -36.856528, lng: 174.758729},
                iconImage: '/images/mapMarkers/dancinghall.png',
                content: '<a href="http://www.nzredbar.co/" target="_blank"><p>Red Bar</p></a>'
            }
            ]
            
            
        // Loop through markers    
        for(var i = 0; i < markers.length; i++){
          addMarker(markers[i]);
        }  
        
        //Add marker function
        function addMarker(props){
            var marker = new google.maps.Marker({
                position: props.coords,
                map: map
            });
          
            // Check for custom icon   
            if(props.iconImage){
               //Set icon image
               marker.setIcon(props.iconImage);
            }
                
            if(props.content){
                var infoWindow = new google.maps.InfoWindow({
                content: props.content 
            });
            
            marker.addListener('click', function(){
                infoWindow.open(map, marker);
            });
            
        
        }
      }
    }
    var markerCluster = new MarkerClusterer(map, markers, {imagePath: '/images/mapMarkers/m1.png'});  <!--// CHECK THIS-->
</script>
<script src="/maps/markerclusterer.js"></script> <!--// CHECK THIS-->
