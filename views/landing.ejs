    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    <div id="landing-header">
 		<h1>Welcome to SLEDGE</h1>
    </div>
    
    <div id="landing-button">
		<a href="/home" class="btn btn-lg btn-primary btn-block">Continue</a>
    </div>
    
    <div id="landing-quote">
		<h2><q>Putting the fun back into your life and providing the power to achieve great results.</q></h2>
    </div>
    
    <div id="fb-root"></div>

    
